<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="itemSize" format="dimension" />
        <attr name="delimiterSize" format="dimension" />
        <attr name="itemIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="GifView">
        <attr name="gifWidth" format="integer"/>
        <attr name="gifHeight" format="integer"/>
        <attr name="repeatCount" format="integer"/>
